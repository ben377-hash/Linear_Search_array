#include <stdio.h>
int linear(long int a[],int n,int key)
{
    for(int i=0;i<n;i++)
    {
        if(a[i]==key)
        {
            return 1;
        }
    }
    return 0;
}
int main() {
   long int a[20];
  int n;
  printf("Enter the size");
  scanf("%d",&n);
   int key;
   printf("Enter the elements :\n");
   for(int i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   printf("Enter the key :");
   scanf("%d",&key);
   
   
  int x=linear(a,n,key);
  if(x==0)
  {
      printf("Not found");
  }
  else
  {
      printf("Found");
  }
   
    return 0;
}
